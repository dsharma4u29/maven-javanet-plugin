<?xml version="1.0"?>
<project>
  <pomVersion>3</pomVersion>
  <id>maven-javanet-plugin</id>
  <name>Maven java.net plugin</name>
  <currentVersion>1.0.1</currentVersion>
  <inceptionYear>2005</inceptionYear>
  <shortDescription>Goals for projects on java.net</shortDescription>
  
  <organization>
    <name>java.net</name>
    <url>http://www.java.net/</url>
    <logo>https://jaxb.dev.java.net/branding/images/header_jnet_new.jpg</logo>
  </organization>
  <issueTrackingUrl>https://maven-javanet-plugin.dev.java.net/servlets/ProjectIssues</issueTrackingUrl>
  <mailingLists>
    <mailingList>
      <name>Users List</name>
      <subscribe>users-subscribe@maven-javanet-plugin.dev.java.net</subscribe>
      <unsubscribe>users-unsubscribe@maven-javanet-plugin.dev.java.net</unsubscribe>
      <archive>https://maven-javanet-plugin.dev.java.net/servlets/SummarizeList?listName=users</archive>
    </mailingList>
    <mailingList>
      <name>Issues List</name>
      <subscribe>issues-subscribe@maven-javanet-plugin.dev.java.net</subscribe>
      <unsubscribe>issues-unsubscribe@maven-javanet-plugin.dev.java.net</unsubscribe>
      <archive>https://maven-javanet-plugin.dev.java.net/servlets/SummarizeList?listName=issues</archive>
    </mailingList>
    <mailingList>
      <name>CVS List</name>
      <subscribe>cvs-subscribe@maven-javanet-plugin.dev.java.net</subscribe>
      <unsubscribe>cvs-unsubscribe@maven-javanet-plugin.dev.java.net</unsubscribe>
      <archive>https://maven-javanet-plugin.dev.java.net/servlets/SummarizeList?listName=cvs</archive>
    </mailingList>
  </mailingLists>
  
  <developers>
    <developer>
      <name>Kohsuke Kawaguchi</name>
      <id>kohsuke</id>
      <email>kk@kohsuke.org</email>
      <roles>
        <role>Developer</role>
      </roles>
    </developer>
  </developers>

  <dependencies>
    <dependency>
      <id>httpunit</id>
      <version>1.5.4</version>
    </dependency>
    <dependency>
      <id>xml-apis</id>
      <version>2.0.0</version>
    </dependency>
    <dependency>
      <id>xerces</id>
      <version>2.2.1</version>
    </dependency>
    <dependency>
      <id>jtidy</id>
      <version>4aug2000r7-dev</version>
    </dependency>
    <dependency>
      <id>nekohtml</id>
      <version>0.7.7</version>
    </dependency>
    <dependency>
      <id>dom4j</id>
      <version>1.4</version>
    </dependency>
    <dependency>
      <id>jaxen</id>
      <version>1.0-FCS-full</version>
    </dependency>
    <!--
    <dependency>
      <id></id>
      <version></version>
    </dependency>
    -->
  </dependencies>

  <build>
    <resources>
      <resource>
        <directory>${basedir}/src/plugin-resources</directory>
        <targetPath>plugin-resources</targetPath>
      </resource>
      <resource>
        <directory>${basedir}</directory>
        <includes>
          <include>plugin.jelly</include>
          <include>plugin.properties</include>
          <include>project.properties</include>
          <include>project.xml</include>
        </includes>
      </resource>
    </resources>
  </build>
</project>